{"ast":null,"code":"export default {\n  data() {\n    return {\n      imageData: null,\n      defectList: [],\n      statsData: [{\n        runTime: null,\n        defectionsSum: null,\n        defectRate: null,\n        highestOccurrenceDefect: null,\n        operation: null,\n        opTime: null\n      }],\n      eventSourcePicture: null\n    };\n  },\n  mounted() {\n    this.initSSEConnection();\n  },\n  beforeDestroy() {\n    if (this.eventSourcePicture) {\n      this.eventSourcePicture.close();\n      console.log('SSEËøûÊé•Â∑≤ÂÖ≥Èó≠');\n    }\n  },\n  methods: {\n    initSSEConnection() {\n      if (this.eventSourcePicture) {\n        this.eventSourcePicture.close();\n      }\n      this.eventSourcePicture = new EventSource('api/dashboard/pictureInfo', {\n        retry: 20000\n      });\n      this.eventSourcePicture.onopen = event => {\n        console.log('‚úÖ SSEËøûÊé•ÊàêÂäü');\n        this.$message.success('ÂÆûÊó∂ËøûÊé•Â∑≤Âª∫Á´ã');\n      };\n      this.eventSourcePicture.onerror = error => {\n        console.error('‚ùå SSEËøûÊé•ÈîôËØØ:', error);\n        // this.$message.error('ËøûÊé•ÂºÇÂ∏∏ÔºåÊ≠£Âú®ÈáçËøû...');\n      };\n      this.eventSourcePicture.onmessage = event => {\n        try {\n          const data = JSON.parse(event.data);\n          const imageBase64 = data.imgBase64;\n          if (imageBase64 !== null && imageBase64 !== undefined && imageBase64 !== '') {\n            this.imageData = imageBase64;\n            this.defectList = data.defections || [];\n            console.log('üì∑ Êî∂Âà∞ÂõæÁâáÊï∞ÊçÆÔºåÁº∫Èô∑Êï∞:', this.defectList.length);\n          }\n          if (data.runTime !== null && data.runTime !== undefined) {\n            this.$nextTick(() => {\n              this.statsData = [{\n                runTime: this.formatRuntime(data.runTime),\n                defectionsSum: data.defectionsSum,\n                defectRate: data.defectRate ? (data.defectRate * 100).toFixed(2) + '%' : '0%',\n                highestOccurrenceDefect: data.highestOccurrenceDefect || 'ÊöÇÊó†',\n                operation: null,\n                opTime: null\n              }];\n              if (data.latestOperations && Array.isArray(data.latestOperations)) {\n                let operations = data.latestOperations.map(op => ({\n                  runTime: null,\n                  defectionsSum: null,\n                  defectRate: null,\n                  highestOccurrenceDefect: null,\n                  operation: op.op || op.operation || 'Êú™Áü•Êìç‰Ωú',\n                  opTime: op.time || op.opTime || '-'\n                }));\n                this.statsData = this.statsData.concat(operations);\n              }\n              this.statsData = this.statsData.filter(obj => Object.values(obj).some(value => value !== null && value !== undefined));\n              console.log('üìä ÁªüËÆ°Êï∞ÊçÆÂ∑≤Êõ¥Êñ∞:', this.statsData);\n            });\n          }\n        } catch (error) {\n          console.error('Ëß£ÊûêSSEÊï∞ÊçÆÂ§±Ë¥•:', error);\n        }\n      };\n    },\n    formatRuntime(seconds) {\n      const hours = Math.floor(seconds / 3600);\n      const minutes = Math.floor(seconds % 3600 / 60);\n      const remainingSeconds = seconds % 60;\n      return `${hours}Â∞èÊó∂${minutes}ÂàÜÈíü${remainingSeconds}Áßí`;\n    },\n    formatTime(timeStr) {\n      if (!timeStr) return '-';\n      try {\n        const date = new Date(timeStr);\n        return date.toLocaleString('zh-CN');\n      } catch (e) {\n        return timeStr;\n      }\n    },\n    sortOpTime(a, b) {\n      const timeA = new Date(a.runTime).getTime();\n      const timeB = new Date(b.runTime).getTime();\n      return timeA - timeB;\n    },\n    getProbabilityType(score) {\n      if (score >= 0.7) return 'danger';\n      if (score >= 0.4) return 'warning';\n      return 'info';\n    },\n    getRowClassName({\n      row,\n      rowIndex\n    }) {\n      if (rowIndex === 0) {\n        return 'summary-row';\n      }\n      return 'operation-row';\n    },\n    Refresh() {\n      console.log('üîÑ ÊâãÂä®Âà∑Êñ∞Êï∞ÊçÆ...');\n      this.$message.info('Ê≠£Âú®Âà∑Êñ∞Êï∞ÊçÆ...');\n      this.initSSEConnection();\n    }\n  }\n};","map":{"version":3,"names":["data","imageData","defectList","statsData","runTime","defectionsSum","defectRate","highestOccurrenceDefect","operation","opTime","eventSourcePicture","mounted","initSSEConnection","beforeDestroy","close","console","log","methods","EventSource","retry","onopen","event","$message","success","onerror","error","onmessage","JSON","parse","imageBase64","imgBase64","undefined","defections","length","$nextTick","formatRuntime","toFixed","latestOperations","Array","isArray","operations","map","op","time","concat","filter","obj","Object","values","some","value","seconds","hours","Math","floor","minutes","remainingSeconds","formatTime","timeStr","date","Date","toLocaleString","e","sortOpTime","a","b","timeA","getTime","timeB","getProbabilityType","score","getRowClassName","row","rowIndex","Refresh","info"],"sources":["src/components/dashboard.vue"],"sourcesContent":["<template>\n  <div class=\"monitoring-dashboard\">\n    <!-- È°∂ÈÉ®Ê†áÈ¢òÂå∫Âüü -->\n    <div class=\"dashboard-header\">\n      <h2 class=\"title\">ÁõëÊéßÂõæÂÉè</h2>\n      <el-button \n        @click=\"Refresh\" \n        type=\"primary\" \n        icon=\"el-icon-refresh\" \n        size=\"small\"\n        class=\"refresh-btn\"\n      >\n        Âà∑Êñ∞Êï∞ÊçÆ\n      </el-button>\n    </div>\n\n    <!-- ‰∏ªË¶ÅÂÜÖÂÆπÂå∫Âüü -->\n    <div class=\"main-content\">\n      <el-row :gutter=\"20\">\n        <!-- Â∑¶‰æßÁõëÊéßÁîªÈù¢ -->\n        <el-col :span=\"12\">\n          <el-card class=\"monitoring-card\" shadow=\"hover\">\n            <div slot=\"header\" class=\"card-header\">\n              <i class=\"el-icon-monitor header-icon\"></i>\n              <span>ÂÆûÊó∂ÁõëÊéßÁîªÈù¢</span>\n            </div>\n            <div class=\"image-container\">\n              <div v-if=\"imageData\" class=\"image-wrapper\">\n                <img \n                  :src=\"'data:image/jpeg;base64,' + imageData\" \n                  alt=\"ÁõëÊéßÂõæÂÉè\" \n                  class=\"monitoring-image\"\n                />\n                <div class=\"image-overlay\">\n                  <span class=\"overlay-text\">ÂÆûÊó∂ÁîªÈù¢</span>\n                </div>\n              </div>\n              <div v-else class=\"no-image\">\n                <i class=\"el-icon-picture-outline no-image-icon\"></i>\n                <p>ÊöÇÊó†ÁõëÊéßÂõæÂÉè</p>\n              </div>\n            </div>\n          </el-card>\n        </el-col>\n\n        <!-- Âè≥‰æßÁº∫Èô∑‰ø°ÊÅØ -->\n        <el-col :span=\"12\">\n          <el-card class=\"defect-card\" shadow=\"hover\">\n            <div slot=\"header\" class=\"card-header\">\n              <i class=\"el-icon-warning-outline header-icon\"></i>\n              <span>Áº∫Èô∑‰ø°ÊÅØ</span>\n              <el-badge \n                :value=\"defectList.length\" \n                :max=\"99\" \n                class=\"defect-badge\"\n                v-if=\"defectList.length > 0\"\n              ></el-badge>\n            </div>\n            <div class=\"table-container\">\n              <el-table \n                :data=\"defectList\" \n                height=\"240\"\n                empty-text=\"ÊöÇÊó†Áº∫Èô∑Êï∞ÊçÆ\"\n                class=\"defect-table\"\n              >\n                <el-table-column \n                  prop=\"category\" \n                  label=\"Áº∫Èô∑ÂêçÁß∞\"\n                  min-width=\"120\"\n                >\n                  <template slot-scope=\"scope\">\n                    <span class=\"defect-name\">{{ scope.row.category }}</span>\n                  </template>\n                </el-table-column>\n                <el-table-column \n                  prop=\"score\" \n                  label=\"Ê¶ÇÁéá\"\n                  width=\"100\"\n                  align=\"center\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-tag \n                      :type=\"getProbabilityType(scope.row.score)\"\n                      size=\"small\"\n                    >\n                      {{ (scope.row.score * 100).toFixed(1) }}%\n                    </el-tag>\n                  </template>\n                </el-table-column>\n              </el-table>\n            </div>\n            <div class=\"total-defects\">\n              <div class=\"total-label\">ÊÄªÁº∫Èô∑Êï∞</div>\n              <div class=\"total-value\">{{ defectList.length }}</div>\n            </div>\n          </el-card>\n        </el-col>\n      </el-row>\n\n      <!-- Â∫ïÈÉ®ÁªüËÆ°‰ø°ÊÅØ -->\n      <div class=\"stats-section\">\n        <el-card class=\"stats-card\" shadow=\"hover\">\n          <div slot=\"header\" class=\"card-header\">\n            <i class=\"el-icon-data-analysis header-icon\"></i>\n            <span>ÁªüËÆ°‰ø°ÊÅØ</span>\n          </div>\n          <div class=\"table-container\">\n            <el-table \n              :data=\"statsData\" \n              height=\"240\"\n              class=\"stats-table\"\n              :row-class-name=\"getRowClassName\"\n            >\n              <el-table-column \n                prop=\"runTime\" \n                label=\"ËøêË°åÊó∂Èïø\" \n                sortable \n                :sort-method=\"sortOpTime\"\n                min-width=\"120\"\n              >\n                <template slot-scope=\"scope\">\n                  <span v-if=\"scope.row.runTime\" class=\"runtime-text\">\n                    {{ scope.row.runTime }}\n                  </span>\n                  <span v-else class=\"empty-text\">-</span>\n                </template>\n              </el-table-column>\n              <el-table-column \n                prop=\"defectionsSum\" \n                label=\"Áº∫Èô∑ÊÄªÊï∞\"\n                width=\"100\"\n                align=\"center\"\n              >\n                <template slot-scope=\"scope\">\n                  <span v-if=\"scope.row.defectionsSum !== null\" class=\"defect-count\">\n                    {{ scope.row.defectionsSum }}\n                  </span>\n                  <span v-else class=\"empty-text\">-</span>\n                </template>\n              </el-table-column>\n              <el-table-column \n                prop=\"defectRate\" \n                label=\"Áº∫Èô∑Áéá\"\n                width=\"100\"\n                align=\"center\"\n              >\n                <template slot-scope=\"scope\">\n                  <span v-if=\"scope.row.defectRate\" class=\"defect-rate\">\n                    {{ scope.row.defectRate }}\n                  </span>\n                  <span v-else class=\"empty-text\">-</span>\n                </template>\n              </el-table-column>\n              <el-table-column \n                prop=\"highestOccurrenceDefect\" \n                label=\"ÊúÄÈ´òÂèëÁº∫Èô∑\"\n                min-width=\"120\"\n              >\n                <template slot-scope=\"scope\">\n                  <span v-if=\"scope.row.highestOccurrenceDefect\" class=\"defect-highlight\">\n                    {{ scope.row.highestOccurrenceDefect }}\n                  </span>\n                  <span v-else class=\"empty-text\">ÊöÇÊó†</span>\n                </template>\n              </el-table-column>\n              <el-table-column \n                prop=\"operation\" \n                label=\"Á≥ªÁªüÊúÄÊñ∞Êìç‰Ωú\"\n                min-width=\"120\"\n              >\n                <template slot-scope=\"scope\">\n                  <span v-if=\"scope.row.operation\" class=\"operation-text\">\n                    {{ scope.row.operation }}\n                  </span>\n                  <span v-else class=\"empty-text\">-</span>\n                </template>\n              </el-table-column>\n              <el-table-column \n                prop=\"opTime\" \n                label=\"Á≥ªÁªüÊúÄÊñ∞Êìç‰ΩúÊó∂Èó¥\"\n                min-width=\"160\"\n              >\n                <template slot-scope=\"scope\">\n                  <span v-if=\"scope.row.opTime\" class=\"time-text\">\n                    {{ formatTime(scope.row.opTime) }}\n                  </span>\n                  <span v-else class=\"empty-text\">-</span>\n                </template>\n              </el-table-column>\n            </el-table>\n          </div>\n        </el-card>\n      </div>\n    </div>\n\n    <!-- ËøûÊé•Áä∂ÊÄÅÊåáÁ§∫Âô® -->\n    <div class=\"connection-status\" :class=\"{'connected': eventSourcePicture && eventSourcePicture.readyState === 1}\">\n      <i class=\"status-icon\" :class=\"eventSourcePicture && eventSourcePicture.readyState === 1 ? 'el-icon-success' : 'el-icon-error'\"></i>\n      <span class=\"status-text\">\n        {{ eventSourcePicture && eventSourcePicture.readyState === 1 ? 'ÂÆûÊó∂ËøûÊé•‰∏≠' : 'ËøûÊé•Êñ≠ÂºÄ' }}\n      </span>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      imageData: null,\n      defectList: [],\n      statsData: [{\n        runTime: null,\n        defectionsSum: null,\n        defectRate: null,\n        highestOccurrenceDefect: null,\n        operation: null,\n        opTime: null\n      }],\n      eventSourcePicture: null\n    }\n  },\n  mounted() {\n    this.initSSEConnection();\n  },\n  beforeDestroy() {\n    if (this.eventSourcePicture) {\n      this.eventSourcePicture.close();\n      console.log('SSEËøûÊé•Â∑≤ÂÖ≥Èó≠');\n    }\n  },\n  methods: {\n    initSSEConnection() {\n      if (this.eventSourcePicture) {\n        this.eventSourcePicture.close();\n      }\n      \n      this.eventSourcePicture = new EventSource('api/dashboard/pictureInfo', { retry: 20000 });\n      \n      this.eventSourcePicture.onopen = (event) => {\n        console.log('‚úÖ SSEËøûÊé•ÊàêÂäü');\n        this.$message.success('ÂÆûÊó∂ËøûÊé•Â∑≤Âª∫Á´ã');\n      };\n      \n      this.eventSourcePicture.onerror = (error) => {\n        console.error('‚ùå SSEËøûÊé•ÈîôËØØ:', error);\n        // this.$message.error('ËøûÊé•ÂºÇÂ∏∏ÔºåÊ≠£Âú®ÈáçËøû...');\n      };\n      \n      this.eventSourcePicture.onmessage = event => {\n        try {\n          const data = JSON.parse(event.data);\n          const imageBase64 = data.imgBase64;\n          \n          if (imageBase64 !== null && imageBase64 !== undefined && imageBase64 !== '') {\n            this.imageData = imageBase64;\n            this.defectList = data.defections || [];\n            console.log('üì∑ Êî∂Âà∞ÂõæÁâáÊï∞ÊçÆÔºåÁº∫Èô∑Êï∞:', this.defectList.length);\n          }\n          \n          if (data.runTime !== null && data.runTime !== undefined) {\n            this.$nextTick(() => {\n              this.statsData = [{\n                runTime: this.formatRuntime(data.runTime),\n                defectionsSum: data.defectionsSum,\n                defectRate: data.defectRate ? (data.defectRate * 100).toFixed(2) + '%' : '0%',\n                highestOccurrenceDefect: data.highestOccurrenceDefect || 'ÊöÇÊó†',\n                operation: null,\n                opTime: null\n              }];\n              \n              if (data.latestOperations && Array.isArray(data.latestOperations)) {\n                let operations = data.latestOperations.map(op => ({\n                  runTime: null,\n                  defectionsSum: null,\n                  defectRate: null,\n                  highestOccurrenceDefect: null,\n                  operation: op.op || op.operation || 'Êú™Áü•Êìç‰Ωú',\n                  opTime: op.time || op.opTime || '-'\n                }));\n                this.statsData = this.statsData.concat(operations);\n              }\n              \n              this.statsData = this.statsData.filter(obj => \n                Object.values(obj).some(value => value !== null && value !== undefined)\n              );\n              \n              console.log('üìä ÁªüËÆ°Êï∞ÊçÆÂ∑≤Êõ¥Êñ∞:', this.statsData);\n            });\n          }\n        } catch (error) {\n          console.error('Ëß£ÊûêSSEÊï∞ÊçÆÂ§±Ë¥•:', error);\n        }\n      };\n    },\n    formatRuntime(seconds) {\n      const hours = Math.floor(seconds / 3600);\n      const minutes = Math.floor((seconds % 3600) / 60);\n      const remainingSeconds = seconds % 60;\n      return `${hours}Â∞èÊó∂${minutes}ÂàÜÈíü${remainingSeconds}Áßí`;\n    },\n    formatTime(timeStr) {\n      if (!timeStr) return '-';\n      try {\n        const date = new Date(timeStr);\n        return date.toLocaleString('zh-CN');\n      } catch (e) {\n        return timeStr;\n      }\n    },\n    sortOpTime(a, b) {\n      const timeA = new Date(a.runTime).getTime();\n      const timeB = new Date(b.runTime).getTime();\n      return timeA - timeB;\n    },\n    getProbabilityType(score) {\n      if (score >= 0.7) return 'danger';\n      if (score >= 0.4) return 'warning';\n      return 'info';\n    },\n    getRowClassName({ row, rowIndex }) {\n      if (rowIndex === 0) {\n        return 'summary-row';\n      }\n      return 'operation-row';\n    },\n    Refresh() {\n      console.log('üîÑ ÊâãÂä®Âà∑Êñ∞Êï∞ÊçÆ...');\n      this.$message.info('Ê≠£Âú®Âà∑Êñ∞Êï∞ÊçÆ...');\n      this.initSSEConnection();\n    }\n  }\n};\n</script>\n\n<style scoped>\n.monitoring-dashboard {\n  padding: 20px;\n  background: linear-gradient(135deg, #f5f7fa 0%, #e4e7ed 100%);\n  min-height: 100vh;\n  height: 100vh;\n  overflow: hidden;\n  display: flex;\n  flex-direction: column;\n}\n\n.dashboard-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n  padding: 0 10px;\n  flex-shrink: 0;\n}\n\n.title {\n  color: #303133;\n  font-size: 24px;\n  font-weight: 600;\n  margin: 0;\n  background: linear-gradient(135deg, #409EFF 0%, #67C23A 100%);\n  -webkit-background-clip: text;\n  -webkit-text-fill-color: transparent;\n}\n\n.refresh-btn {\n  border-radius: 16px;\n  padding: 8px 16px;\n}\n\n.main-content {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n  overflow: hidden;\n}\n\n/* Âç°ÁâáÈÄöÁî®Ê†∑Âºè */\n.m